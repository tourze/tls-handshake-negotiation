{
  "name": "tourze/tls-handshake-negotiation",
  "description": "TLS握手协商过程的核心组件，包括扩展处理、证书选择、密钥交换和PSK会话管理",
  "type": "library",
  "license": "MIT",
  "require": {
    "ext-hash": "*",
    "ext-openssl": "*",
    "tourze/enum-extra": "1.0.*",
    "tourze/tls-common": "0.0.*",
    "tourze/tls-crypto-keyexchange": "0.0.*",
    "tourze/tls-handshake-messages": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/framework-bundle": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-enum": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\TLSHandshakeNegotiation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\TLSHandshakeNegotiation\\Tests\\": "tests/"
    }
  }
}